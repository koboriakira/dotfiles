{
  "globalShortcut": "Shift+Alt+Space",
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--vision"
      ]
    },
    "screening-test": {
      "command": "uv",
      "args": [
        "run",
        "--directory",
        "/Users/koboriakira/git/screening-test",
        "python",
        "-m",
        "screening_test.mcp_server"
      ]
    }
  },
  "preferences": {
    "coworkScheduledTasksEnabled": false,
    "sidebarMode": "chat",
    "bypassPermissionsModeEnabled": true
  }
}
